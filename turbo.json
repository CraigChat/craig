{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "outputs": [
        ".svelte-kit/**",
        "build/**",
        "dist/**"
      ],
      "dependsOn": [
        "^build"
      ]
    },
    "lint": {},
    "format": {},
    "generate": {},
    "dev": {
      "cache": false,
      "persistent": true,
      "dependsOn": [
        "^generate"
      ]
    },
    "inspect": {
      "cache": false,
      "persistent": true,
      "dependsOn": [
        "^build"
      ]
    }
  }
}
