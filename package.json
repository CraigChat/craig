{
  "name": "craig",
  "version": "0.0.0",
  "homepage": "http://craig.chat/",
  "license": "ISC",
  "private": true,
  "author": {
    "name": "Snazzah",
    "email": "me@snazzah.com",
    "url": "https://snazzah.com/"
  },
  "engines": {
    "node": ">=18.20.4",
    "pnpm": ">=9"
  },
  "packageManager": "pnpm@9.1.1",
  "scripts": {
    "dev": "turbo dev --no-daemon",
    "inspect": "turbo inspect",
    "build": "turbo build",
    "lint": "turbo lint",
    "format": "turbo format",
    "build-cook": "pnpm -F kitchen -F ennuizel-streamer run build-cook",
    "sync": "pnpm -F bot run sync",
    "sync:dev": "pnpm -F bot run sync:dev",
    "db:deploy": "dotenv -c -- pnpm -F db run db:migrate:deploy"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CraigChat/craig"
  },
  "dependencies": {
    "dotenv-cli": "8.0.0",
    "eslint": "9.21.0",
    "eslint-config-craig": "workspace:*",
    "rimraf": "5.0.10",
    "turbo": "2.5.3"
  },
  "pnpm": {
    "overrides": {
      "supports-color": "8.1.1"
    },
    "patchedDependencies": {
      "svelte-awesome-color-picker@4.0.2": "patches/svelte-awesome-color-picker@4.0.2.patch"
    }
  }
}
