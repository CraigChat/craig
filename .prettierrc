{
  "arrowParens": "always",
  "printWidth": 150,
  "semi": true,
  "singleQuote": true,
  "tabWidth": 2,
  "useTabs": false,
  "trailingComma": "none",
  "bracketSameLine": false,
  "svelteAllowShorthand": true,
  "svelteIndentScriptAndStyle": true,
	"overrides": [{ "files": "*.svelte", "options": { "parser": "svelte" } }],
  "plugins": [
    "prettier-plugin-svelte",
    "prettier-plugin-tailwindcss"
  ]
}
