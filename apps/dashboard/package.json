{
  "name": "@craig/dashboard",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "dotenv -e ../../.env -- vite dev --host --clearScreen false",
    "build": "dotenv -e ../../.env -e .env -c -- vite build",
    "preview": "dotenv -e ../../.env -e .env -c -- vite preview --port 47131",
    "start": "dotenv -e ../../.env -e .env -c -- node build",
    "prepare": "svelte-kit sync || echo ''",
    "start-pm2": "dotenv -e ../../.env -e .env -c -- pm2 start ecosystem.config.cjs",
    "reload-pm2": "dotenv -e ../../.env -e .env -c -- pm2 reload ecosystem.config.cjs --update-env",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint ."
  },
  "devDependencies": {
    "@craig/types": "workspace:*",
    "@floating-ui/core": "^1.7.3",
    "@floating-ui/dom": "^1.7.4",
    "@fontsource-variable/inter": "^5.2.8",
    "@iconify-icons/file-icons": "^1.2.3",
    "@iconify-icons/logos": "^1.2.36",
    "@iconify-icons/material-symbols": "^1.2.58",
    "@iconify-icons/mdi": "^1.2.48",
    "@iconify-icons/simple-icons": "^1.2.74",
    "@iconify-icons/twemoji": "^1.2.8",
    "@iconify/svelte": "^5.2.1",
    "@prgm/sveltekit-progress-bar": "^3.0.2",
    "@sveltejs/adapter-auto": "^6.1.1",
    "@sveltejs/enhanced-img": "^0.8.5",
    "@sveltejs/kit": "^2.49.5",
    "@sveltejs/vite-plugin-svelte": "^6.2.4",
    "@tailwindcss/vite": "^4.1.18",
    "@types/jsonwebtoken": "^9.0.10",
    "bits-ui": "^2.15.4",
    "clsx": "^2.1.1",
    "cookie-es": "^2.0.0",
    "discord-api-types": "^0.38.37",
    "jsonwebtoken": "^9.0.3",
    "nanoid": "^5.1.6",
    "prettier": "^3.8.0",
    "prettier-plugin-svelte": "^3.4.1",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "svelte": "^5.46.4",
    "svelte-check": "^4.3.5",
    "svelte-floating-ui": "^1.6.2",
    "svelte-i18n": "^4.0.1",
    "svelte-persisted-store": "^0.12.0",
    "svelte-portal": "^2.2.1",
    "svelte-sonner": "^1.0.7",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18",
    "tailwindcss-animate": "^1.0.7",
    "trap-focus-svelte": "^1.1.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vite-plugin-devtools-json": "^0.2.1"
  },
  "dependencies": {
    "@craig/db": "workspace:*",
    "@craig/logger": "workspace:*",
    "@henrygd/semaphore": "^0.1.0",
    "@sveltejs/adapter-node": "^5.5.1",
    "destr": "^2.0.5",
    "dropbox": "^10.34.0",
    "googleapis": "^149.0.0",
    "ioredis": "^5.9.2",
    "just-split": "^3.2.0",
    "tweetnacl": "^1.0.3",
    "zod": "^4.3.5"
  }
}
