{
  "name": "@craig/ferret",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "dotenv -e ../../.env -e .env -c -- node build",
    "inspect": "dotenv -e ../../.env -e .env -c -- node --inspect=7002 build",
    "dev": "dotenv -e ../../.env -e .env -c -- vite dev --host",
    "build": "dotenv -e ../../.env -e .env -c -- vite build",
    "preview": "dotenv -e ../../.env -e .env -c -- vite preview --host",
    "start-pm2": "dotenv -e ../../.env -e .env -c -- pm2 start ecosystem.config.cjs",
    "reload-pm2": "dotenv -e ../../.env -e .env -c -- pm2 reload ecosystem.config.cjs --update-env",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint .",
    "format": "eslint . --fix"
  },
  "dependencies": {
    "@craig/db": "workspace:*",
    "@craig/logger": "workspace:*",
    "@henrygd/queue": "^1.0.7",
    "@libav.js/variant-webcodecs": "^6.8.8",
    "@wasm-audio-decoders/flac": "^0.2.8",
    "destr": "^2.0.5",
    "emittery": "^1.2.0",
    "eventemitter3": "^5.0.1",
    "ioredis": "^5.9.2",
    "just-clone": "^6.2.0",
    "mediabunny": "^1.21.1",
    "nanoid": "^5.1.6",
    "opus-decoder": "^0.7.10",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@craig/types": "workspace:*",
    "@floating-ui/core": "^1.7.3",
    "@floating-ui/dom": "^1.7.4",
    "@fontsource-variable/inter": "^5.2.8",
    "@fontsource-variable/roboto-mono": "^5.2.8",
    "@iconify-icons/cib": "^1.2.4",
    "@iconify-icons/file-icons": "^1.2.3",
    "@iconify-icons/mdi": "^1.2.48",
    "@iconify-icons/radix-icons": "^1.2.9",
    "@iconify-icons/simple-icons": "^1.2.74",
    "@iconify-icons/twemoji": "^1.2.8",
    "@iconify/svelte": "^5.2.1",
    "@sveltejs/adapter-node": "^5.5.1",
    "@sveltejs/enhanced-img": "^0.8.5",
    "@sveltejs/kit": "^2.49.5",
    "@sveltejs/vite-plugin-svelte": "^6.2.4",
    "@types/wicg-file-system-access": "^2023.10.6",
    "autoprefixer": "^10.4.23",
    "clsx": "^2.1.1",
    "cookie-es": "^2.0.0",
    "discord-api-types": "^0.38.37",
    "fflate": "^0.8.2",
    "just-range": "^4.2.0",
    "mime": "^4.1.0",
    "postcss": "^8.5.6",
    "postcss-load-config": "^6.0.1",
    "sass": "^1.97.2",
    "svelte": "^5.46.4",
    "svelte-awesome-color-picker": "4.0.2",
    "svelte-check": "^4.3.5",
    "svelte-floating-ui": "^1.6.2",
    "svelte-i18n": "^4.0.1",
    "svelte-persisted-store": "^0.12.0",
    "svelte-portal": "^2.2.1",
    "tailwindcss": "^3.4.19",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vite-plugin-devtools-json": "^1.0.0"
  }
