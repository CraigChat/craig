{
  "name": "@craig/ferret",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "dotenv -e ../../.env -e .env -c -- node build",
    "inspect": "dotenv -e ../../.env -e .env -c -- node --inspect=7002 build",
    "dev": "dotenv -e ../../.env -e .env -c -- vite dev --host",
    "build": "dotenv -e ../../.env -e .env -c -- vite build",
    "preview": "dotenv -e ../../.env -e .env -c -- vite preview",
    "start-pm2": "dotenv -e ../../.env -e .env -c -- pm2 start ecosystem.config.cjs",
    "reload-pm2": "dotenv -e ../../.env -e .env -c -- pm2 reload ecosystem.config.cjs --update-env",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint .",
    "format": "eslint . --fix"
  },
  "dependencies": {
    "@craig/logger": "workspace:*",
    "destr": "^2.0.5",
    "emittery": "^1.1.0",
    "eventemitter3": "^5.0.1",
    "ioredis": "^5.6.1",
    "just-clone": "^6.2.0",
    "nanoid": "^5.1.5",
    "zod": "^3.25.20"
  },
  "devDependencies": {
    "@craig/types": "workspace:*",
    "@floating-ui/core": "^1.7.0",
    "@floating-ui/dom": "^1.7.0",
    "@fontsource-variable/inter": "^5.2.5",
    "@fontsource-variable/roboto-mono": "^5.2.5",
    "@iconify-icons/cib": "^1.2.4",
    "@iconify-icons/file-icons": "^1.2.3",
    "@iconify-icons/mdi": "^1.2.48",
    "@iconify-icons/simple-icons": "^1.2.74",
    "@iconify-icons/twemoji": "^1.2.8",
    "@iconify/svelte": "^5.0.0",
    "@sveltejs/adapter-node": "^5.2.12",
    "@sveltejs/enhanced-img": "^0.6.0",
    "@sveltejs/kit": "^2.21.1",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "autoprefixer": "^10.4.21",
    "clsx": "^2.1.1",
    "cookie-es": "^2.0.0",
    "discord-api-types": "^0.38.8",
    "fflate": "^0.8.2",
    "just-range": "^4.2.0",
    "postcss": "^8.5.3",
    "postcss-load-config": "^6.0.1",
    "sass": "^1.89.0",
    "svelte": "^5.32.1",
    "svelte-awesome-color-picker": "^4.0.2",
    "svelte-check": "^4.2.1",
    "svelte-floating-ui": "^1.6.2",
    "svelte-i18n": "^4.0.1",
    "svelte-persisted-store": "^0.12.0",
    "svelte-portal": "^2.2.1",
    "tailwindcss": "^3",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3",
    "vite": "^6.3.5"
  }
}
