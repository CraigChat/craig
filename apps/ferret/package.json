{
  "name": "@craig/ferret",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "dotenv -e ../../.env -e .env -c -- node build",
    "dev": "dotenv -e ../../.env -e .env -c -- vite dev --host",
    "build": "dotenv -e ../../.env -e .env -c -- vite build",
    "preview": "dotenv -e ../../.env -e .env -c -- vite preview",
    "start-pm2": "dotenv -e ../../.env -e .env -c -- pm2 start ecosystem.config.cjs",
    "reload-pm2": "dotenv -e ../../.env -e .env -c -- pm2 reload ecosystem.config.cjs --update-env",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint .",
    "format": "eslint . --fix"
  },
  "dependencies": {
    "@craig/logger": "workspace:*",
    "destr": "^2.0.3",
    "emittery": "^1.0.3",
    "eventemitter3": "^5.0.1",
    "ioredis": "^5.4.2",
    "just-clone": "^6.2.0",
    "nanoid": "^5.0.9",
    "svelte-persisted-store": "^0.12.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@craig/types": "workspace:*",
    "@floating-ui/core": "^1.6.9",
    "@floating-ui/dom": "^1.6.13",
    "@fontsource-variable/inter": "^5.1.1",
    "@fontsource-variable/roboto-mono": "^5.1.1",
    "@iconify-icons/cib": "^1.2.4",
    "@iconify-icons/file-icons": "^1.2.3",
    "@iconify-icons/mdi": "^1.2.48",
    "@iconify-icons/simple-icons": "^1.2.74",
    "@iconify-icons/twemoji": "^1.2.8",
    "@iconify/svelte": "^4.2.0",
    "@sveltejs/adapter-node": "^5.2.12",
    "@sveltejs/enhanced-img": "^0.4.4",
    "@sveltejs/kit": "^2.16.0",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "autoprefixer": "^10.4.20",
    "clsx": "^2.1.1",
    "cookie-es": "^2.0.0",
    "discord-api-types": "^0.37.116",
    "fflate": "^0.8.2",
    "just-range": "^4.2.0",
    "postcss": "^8.5.1",
    "postcss-load-config": "^6.0.1",
    "sass": "^1.83.4",
    "svelte": "^5.19.0",
    "svelte-awesome-color-picker": "^4.0.0",
    "svelte-check": "^4.1.4",
    "svelte-floating-ui": "^1.5.9",
    "svelte-i18n": "^4.0.1",
    "svelte-portal": "^2.2.1",
    "tailwindcss": "^3.4.17",
    "tslib": "^2.6.2",
    "typescript": "^5.7.3",
    "vite": "^6.0.7"
  }
}
